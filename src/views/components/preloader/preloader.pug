mixin preloader(props = {})
  - const rootClassName = "preloader";
  - const className = props.class || null;
  - const style = props.style || null;
  -
    const classes = [
      rootClassName,
      className
    ];

  div(
    class = classes 
    style = style
  )
    div(class = `${rootClassName}__circle`)
      span
      span